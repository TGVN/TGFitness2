	<script>
		$(document).ready(function() {
			$(document).on( "click", ".frame_youtube", function(e) {
				$(this).addClass('active');
				var youtube_id = $(this).attr('youtube_id');
                $(this).html('<iframe src="https://www.youtube.com/embed/' + youtube_id + '?autoplay=1&rel=0&showinfo=0" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" referrerpolicy="strict-origin-when-cross-origin" allowfullscreen></iframe>');
			});

			phantrang();
			function phantrang(){
			    var itemsPerPage = 3; // Số khóa học trên mỗi trang
			    var $items = $('.box_giaovien'); // Chọn tất cả các khóa học
			    var numItems = $items.length; // Đếm tổng số khóa học
			    var numPages = Math.ceil(numItems / itemsPerPage); // Tính tổng số trang

			    // Ẩn tất cả các khóa học, chỉ hiển thị các khóa học của trang đầu tiên
			    $items.hide().slice(0, itemsPerPage).show();

			    // Tạo các nút phân trang
			    $('#pagination').pagination({
			        items: numItems,
			        itemsOnPage: itemsPerPage,
			        cssStyle: 'light-theme',
			        prevText: '❮', // Tùy chỉnh nút "Prev"
			        nextText: '❯', // Tùy chỉnh nút "Next"

			        onPageClick: function (pageNumber) {
			            // Ẩn tất cả các khóa học và chỉ hiển thị các khóa học của trang được chọn
			            var start = (pageNumber - 1) * itemsPerPage;
			            var end = start + itemsPerPage;
			            $items.hide().slice(start, end).show();
			            $('body,html').animate({scrollTop:$('#list_giaovien').offset().top}, 300);
			            $('.slide_list').slick('refresh');
			        }
			    });
			}
            initializeSlider();
            function initializeSlider() {
                $('.slide_list').slick({
					infinite: true,
					slidesToShow: 1,
					slidesToScroll: 1,
					arrows: true
                });
                $('.slide_giaovien').slick({
					infinite: true,
					slidesToShow: 1,
					slidesToScroll: 1,
					pauseOnHover: true,
					dots: true
                });
            }
		});
	</script>
