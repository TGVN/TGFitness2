	<script>
		$(document).ready(function() {
            function setEqualHeight() {
                var currentTallest = 0,
                    currentRowStart = 0,
                    rowDivs = [],
                    $el,
                    topPosition = 0;

                $('.box_thucpham > div .thucpham_thongtin').each(function() {
                    $el = $(this);
                    $el.height('auto');
                    topPosition = $el.position().top;

                    if (currentRowStart != topPosition) {
                        // Set all heights to the max height of the row
                        for (var i = 0; i < rowDivs.length; i++) {
                            rowDivs[i].height(currentTallest);
                        }
                        // Set new row parameters
                        rowDivs.length = 0; // Empty the array
                        currentRowStart = topPosition;
                        currentTallest = $el.height();
                        rowDivs.push($el);
                    } else {
                        rowDivs.push($el);
                        currentTallest = (currentTallest < $el.height()) ? ($el.height()) : (currentTallest);
                    }

                    // Set the height of the last row
                    for (var i = 0; i < rowDivs.length; i++) {
                        rowDivs[i].height(currentTallest);
                    }
                });
            }

            setEqualHeight();
            $(window).resize(function(event) {
            	setEqualHeight();
            });
            $(document).on( "click", ".box_thucpham > div", function(e) {
            	// var content = $(this).html();
            	// $('.box_thucpham_chitiet > div').html(content);
            	// $('.box_thucpham_chitiet').show();
            	// $('body').css('overflow','hidden');
            });
            $(document).on( "click", ".close_thucpham_chitiet", function(e) {
            	$('.box_thucpham_chitiet > div').html('');
            	$('.box_thucpham_chitiet').hide();
            	// $('body').css('overflow','auto');
            });
            $(document).on( "click", ".filter_thucpham a", function(e) {
            	$(this).toggleClass('active');
            	locthucpham();
            });
			function locthucpham() {
			    // Lấy danh sách các bộ lọc đang được chọn
			    var filter = [];
			    $('.filter_thucpham a.active').each(function(index, el) {
			        filter.push($(this).attr('filter'));
			    });
			    console.log(filter.length);
			    // Xử lý các div trong .box_thucpham
			    $('.box_thucpham > div').each(function() {
			        var divFilters = $(this).attr('filter').split(',');
			        
			        // Kiểm tra xem thuộc tính filter của div có giao nhau với các bộ lọc đang chọn không
			        var hasActiveFilter = filter.some(function(f) {
			            return divFilters.includes(f);
			        });
			        
			        // Thêm hoặc xóa class 'active' dựa trên việc có bộ lọc nào khớp không
			        if (hasActiveFilter) {
			            $(this).addClass('active');
			        } else {
			            $(this).removeClass('active');
			        }

			    });
			    if(filter.length==0){
			    	$('.box_thucpham > div').addClass('active');
			    }
			}

		});
	</script>
</body>
