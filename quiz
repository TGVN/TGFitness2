	<script>
		$(document).ready(function() {
			var rangeSlider = function(){
			  var slider = $('.range-slider'),
			      range = $('.range-slider__range'),
			      value = $('.range-slider__value');
			    
			  slider.each(function(){

			    value.each(function(){
			      var value = $(this).prev().attr('value');
			      $(this).html(value);
			    });

			    range.on('input', function(){
			      $(this).next(value).html(this.value);
			    });
			  });
			};

			rangeSlider();
			$(document).on( "change", "input[type=radio],input[type=checkbox]", function(e) {
				$('input[type=radio],input[type=checkbox]').each(function() {
					if ($(this).is(':checked')) {
						$(this).closest('label').addClass('selected');
					}else{
						$(this).closest('label').removeClass('selected');
					}
				});
			});
			$(document).on( "click", "input[name=tapluyen_text],input[name=khokhan_text]", function(e) {
				$(this).closest('label').addClass('selected');
			});
            $('.slide_noibat').slick({
				  infinite: true,
				  slidesToShow: 1,
				  slidesToScroll: 1,
				  dots: true,
				  arrows: true,
				  autoplay: true
            });
			$(document).on( "click", ".quiz_submit", function(e) {
				$('.loading').show();
				$('*').removeClass('loi');
				var hoten = $('input[name=hoten]').val();
				var dienthoai = $('input[name=dienthoai]').val();
				var chieucao = $('input[name=chieucao]').val();
				var tuoi = $('input[name=tuoi]').val();
				var cannang_hientai = $('input[name=cannang_hientai]').val();
				var cannang_mongmuon = $('input[name=cannang_mongmuon]').val();

				var muctieu=[];
	            $('input[name="muctieu"]:checked').each(function() {
	                muctieu.push($(this).val());
	            });
				var muctieu_text = $('input[name=muctieu_text]').val();
				if(muctieu_text!=''){
					muctieu.push(muctieu_text);		
				}
				muctieu = muctieu.join(',');



				var tapluyen1 = $('input[name=tapluyen]:checked').val();
				var tapluyen2 = $('input[name=tapluyen_text]').val();
				var tapluyen = [];
				if(tapluyen1){
					tapluyen.push(tapluyen1);
				}
				if(tapluyen2!=''){
					tapluyen.push(tapluyen2);
				}
				tapluyen = tapluyen.join(',');
				var khokhan=[];
	            $('input[name="khokhan"]:checked').each(function() {
	                khokhan.push($(this).val());
	            });
				var khokhan_text = $('input[name=khokhan_text]').val();
				if(khokhan_text!=''){
					khokhan.push(khokhan_text);		
				}
				khokhan = khokhan.join(',');
				var body_quiz = '';
				if(hoten==''){
					$('input[name=hoten]').addClass('loi');
					$('html,body').animate({scrollTop:$('#hoten').offset().top -50}, 400);
					$('input[name=hoten]').focus();
					setTimeout(function() { $('.loading').hide(); }, 500);
				}else if(dienthoai==''){
					$('input[name=dienthoai]').addClass('loi');
					$('html,body').animate({scrollTop:$('#dienthoai').offset().top -50}, 400);
					$('input[name=dienthoai]').focus();
					setTimeout(function() { $('.loading').hide(); }, 500);
				}else if(muctieu==''){
					$('#muctieu label').addClass('loi');
					$('html,body').animate({scrollTop:$('#muctieu').offset().top -50}, 400);
					setTimeout(function() { $('.loading').hide(); }, 500);
				}else if(tapluyen==''){
					$('#tapluyen label').addClass('loi');
					$('html,body').animate({scrollTop:$('#tapluyen').offset().top -50}, 400);
					setTimeout(function() { $('.loading').hide(); }, 500);
				}else if(khokhan==''){
					$('#khokhan label').addClass('loi');
					$('html,body').animate({scrollTop:$('#khokhan').offset().top -50}, 400);
					setTimeout(function() { $('.loading').hide(); }, 500);
				}else{
					var bmi_hientai = parseInt(cannang_hientai) / ((parseInt(chieucao)/100)*(parseInt(chieucao)/100));
					var bmi_danhgia = '';
					if(bmi_hientai<18){
						bmi_danhgia = 'Bạn đang gầy';
					}else if(bmi_hientai>=18 && bmi_hientai<22){
						bmi_danhgia = 'Bạn bình thường';
					}else if(bmi_hientai>=22 && bmi_hientai<35){
						bmi_danhgia = 'Bạn đang thừa cân';
					}else{
						bmi_danhgia = 'Bạn khá béo tròn';
					}

					var cannang_lytuong = 45.5+(2.1*(parseInt(chieucao)-152.4))/2.54;
					var cannang_lytuong1 = cannang_lytuong - 2;
					var cannang_lytuong2 = cannang_lytuong + 1;
					cannang_lytuong = '<table><tr><td><small>Từ &nbsp;</small></td><td>'+cannang_lytuong1.toFixed(2)+'</td><td><small>&nbsp; kg</small></td></tr><tr><td><small>Đến &nbsp;</small></td><td>'+cannang_lytuong2.toFixed(2)+'</td><td><small>&nbsp; kg</small></td></tr></table>';
					cannanglytuong = cannang_lytuong1.toFixed(2)+'-'+cannang_lytuong2.toFixed(2)
					if(parseInt(chieucao)<152.4){
						cannang_lytuong = '45.5 <small>kg</small>';
					}

					var luongmo = 1.2*bmi_hientai+0.23*parseInt(tuoi)-5.4;
					var luongmo_danhgia = '';
					if(luongmo>24){
						luongmo_danhgia = "Bạn có thể bị thừa mỡ, cơ thể cần đốt mỡ";
					}else if(luongmo>=21 && luongmo<=24){
						luongmo_danhgia = "Bạn có lượng mỡ cơ thể lành mạnh";
					}
					body_quiz = `
						<div>
							<table cellpadding="0" cellspacing="0" width="100%">
								<tr>
									<td width="40%"><img src="img/bodyquiz.jpg" width="100%"></td>
									<td width="10">&nbsp;</td>
									<td class="txt_center">
										<span class="danhgia" style="color:#532f4c;">Kết quả body của bạn</span>
										<h2 class="quiz_ketqua">`+hoten+`</h2><br><br>
										<h3>BMI hiện tại</h3>
										<div class="quiz_ketqua">`+bmi_hientai.toFixed(2)+`</div><br>
										<span class="danhgia">`+bmi_danhgia+`</span><br>
										<h3>Cân nặng lý tưởng</h3>
										<div class="quiz_ketqua">`+cannang_lytuong+`</div><br><br>
										<h3>Lượng mỡ cơ thể</h3>
										<div class="quiz_ketqua">`+luongmo.toFixed(2)+`<sup>%</sup></div><br>
										<span class="danhgia">`+luongmo_danhgia+`</span>
									</td>
								</tr>
							</table>
						</div><br>
						<div>
							Hãy bắt đầu hành trình độ dáng cùng Shefit với các bài tập và menu ăn kiêng trong 28 ngày! 
						</div>
					`;
					$('#box_quiz').html(body_quiz);
					$('#slide_quiz').show();
					$('.slide_noibat').slick('refresh');
			        $.ajax({
			            url: 'https://script.google.com/macros/s/AKfycbyiiDlbnqxFWbMCxK_BqMUP0jXDlziwnt7RaScIMEOllicNyAmYvbL2-9x-O4e70FoD/exec?',
			            type: 'GET',
			            data: {
			            	'hoten': hoten,
			            	'dienthoai': dienthoai,
			            	'chieucao': chieucao,
			            	'tuoi': tuoi,
			            	'cannanghientai': cannang_hientai,
			            	'cannangkivong': cannang_mongmuon,
			            	'muctieu': muctieu,
			            	'tapluyen': tapluyen,
			            	'khokhan': khokhan,
			            	'ngay': "22:23:37 29/10/2024"
			            },
			            success: function (response) {
			            	console.log(cannang_hientai);
					        $.ajax({
					            url: 'post.php',
					            type: 'POST',
					            data: {
					            	'update_hocvien': '',
					            	'hocvien_cannang': cannang_hientai,
					            	'hocvien_chieucao': chieucao,
					            	'hocvien_muctieu': muctieu
					            },
					            success: function (response) {
									$('html,body').animate({scrollTop:0}, 400);
									setTimeout(function() { $('.loading').hide(); }, 5000);
					            },
					            error: function (e) {
					                console.log(e.message);
					            }
					        });
			            },
			            error: function (e) {
			                console.log(e.message);
			            }
			        });

					setTimeout(function() { $('.loading').hide(); }, 5000);
				}
